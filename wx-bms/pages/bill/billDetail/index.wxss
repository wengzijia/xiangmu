/* pages/bill/billDetail/index.wxss */
@import '../../../bilcommonl.wxss';
.search {
  width: 100%;
  box-sizing: border-box;
  background-color: #EEEEEE;
  height: 68rpx;
  line-height: 68rpx;
  border-radius: 48rpx;
  display: flex;
  align-items: center;

}

.search input {
  color: #333;
  margin-right: 30rpx;
}

.sear_img image {
  width: 36rpx;
  height: 36rpx;
  vertical-align: middle;
}

.sear_img {
  margin: 0 20rpx 0 40rpx;
}

.placeholder-text {
  font-size: 28rpx;
  font-size: #999;
}



.search_all {
  padding: 20rpx 40rpx 20rpx;
}





.order_flex {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 0 18rpx;
  border-bottom: 1px solid #eee;
}






.listname {
  margin: 16rpx 0 18rpx;
}






.bot_color {
  padding-bottom: 16rpx;
  font-size: 28rpx;
  margin-right: -50rpx;
}




.bot_color2 {
  padding: 16rpx 0;
  font-size: 28rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.edit_btn {
  font-size: 30rpx;
  height: 56rpx;
  line-height: 56rpx;
  border-radius: 34rpx;
  color: #E9333F;
  padding: 0rpx 24rpx;
  border: 1px solid #E9333F
}

.bcolor {
  color: #333;
}

.pcolor {
  color: #E9333F;
}

.countcolor {
  color: #E9333F;
  font-size: 32rpx;
  font-weight: bold;
}

.bot_color text,
.bot_b text {
  vertical-align: middle;
}

.share_ab image {
  width: 60rpx;
  height: 60rpx;
}

.share_ab {
  position: absolute;
  right: -1rpx;
  top: -1rpx;
  z-index: 2;
  width: 60rpx;
  height: 60rpx;
  margin: 0;
  padding: 0;
  border-radius: 0rpx 20rpx 0rpx 20rpx;
  background-size: 60rpx 60rpx;
  background-repeat: no-repeat;
  background-image: url(data:image/png;base64,);
}





.none_all {
  margin: 90rpx auto 0;
  text-align: center;
}

