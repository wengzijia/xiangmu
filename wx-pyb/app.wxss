/**app.wxss**/
@import './fonts.wxss';

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}

.red {
  color: #FF0036;
}

button {
  display: inline-block;
}

button::after {
  border: 0;
}

.btuBottom {
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}

.order_flex {
  font-size: 28rpx;
  /* margin-bottom: 20rpx; */
  line-height: 1.5;
  padding-bottom: 20rpx;
}

.order_front {
  color: #999;
}

.order_after {
  color: #666;
}
image,.daomo-img.image-vivid{
  image-rendering: -moz-crisp-edges; /* Firefox */
  image-rendering: -o-crisp-edges; /* Opera */
  image-rendering: -webkit-optimize-contrast; /* Webkit (non-standard naming) */
  image-rendering: crisp-edges;
  -ms-interpolation-mode: nearest-neighbor; /* IE (non-standard property) */
}
.sidebar-imgbox image,.daomo-img{
  image-rendering: -moz-crisp-edges; /* Firefox */
  image-rendering: -o-crisp-edges; /* Opera */
  image-rendering: -webkit-optimize-contrast; /* Webkit (non-standard naming) */
  image-rendering:auto;
  -ms-interpolation-mode: nearest-neighbor; /* IE (non-standard property) */
}
.tabs1 .grid1 {
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
  color: #fff;
}

.tabs1 .grid {
  background: url(data:image/png;base64,);
  background-size: 100% 100%;
  color: #fff;
}

/* 信用支付 */
.credit-action-sheet button {
  display: block;
}

.parcel {
  width: 102rpx;
  text-align: center;
  border-radius: 8rpx;
  margin-right: 10rpx;
  font-size: 28rpx;
  height: 40rpx;
  line-height: 40rpx;
}

.gcolor {
  color: #B98A12;
  background-color: #FFF4D0;
}

.pcolor {
  color: #E9333F;
  background-color: #FFE5E7;
}

.action-sheet {
  padding: 8px 16px;
  background-color: #fff;
  box-sizing: border-box;
}

.credit-action {
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 32rpx;
}

.credit-icon {
  width: 28px;
  height: 28 px;
  margin-right: 8px;
}

.subname {
  text-align: center;
  color: #999;
  font-size: 26rpx;
  line-height: 26rpx;
}

.wid_img {
  width: 50rpx;
  height: 50rpx;
}

.loading-text {
  font-size: 30rpx;
  text-align: center;
  clear: #333;
  margin: 50rpx auto;
}

