.textAtive {
  max-height: none;
  word-break: break-all;
}
.textBtn {
  position: relative;
  float: right;
  clear: both;
  font-size: 28rpx;
  color: #238BEB;
  line-height: 36rpx;
  height: 30rpx;
}
.textBtn2 {
  font-size: 28rpx;
  text-align: right;
}
.textShow {
  display: inline-block;
  color: #238BEB;
}
.textNone {
  display: none;
}
.textcolor {
  color: #999;
}
.showDot {
  word-break: break-all;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box !important;
  -webkit-line-clamp: 4;
  -webkit-box-orient: vertical;
}
.textBtn1{
  float: right;
  clear: both;
  height: 1.4em;
  line-height: 1.4em;
}
.alltext{
  position: absolute;
  right: 0;
  bottom: 0;
  background: #fff;
  text-align: right;
  font-size: 28rpx;
  color: #238BEB;
}
.alltext::before{
  content: ""; 
  padding-left: 0.5em;
}
