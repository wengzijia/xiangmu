page {
  background-color: #f4f4f4;
}
.body view {
  background-color: #fff;
}
.personage {
  /* padding: 0 20rpx; */
  position: relative;  
}
.head{
  z-index: 99;
}
button{
  padding: 0;
}
.ac-icon-del{
  vertical-align: -11%;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  background: #ff0036; 
  border: none;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before{
  color: #fff;
}
checkbox .wx-checkbox-input{
  border-radius: 50%;
}
checkbox{
  width: 50rpx;
  height: 50rpx;
}
.clear{
  width: 54rpx;
  height: 54rpx;
}
.tabs {
  font-size:32rpx;
  line-height:86rpx;
  border-top:1rpx solid #f5f5f5;
  border-bottom:1rpx solid #e5e5e5;
  display: flex;
  justify-content: space-between;
  color:#333;
}
.tabs > text {
  display: inline-block;
  width: 20%;
  text-align: center;
  margin:0 24rpx;
}
.grid {
  border-bottom: orangered 4rpx solid;
  color:#ff0036;
}
.order {
  margin-bottom: 24rpx;
  border-bottom: 1rpx solid #e7e7e7;
  border-top: 1rpx solid #e7e7e7;
}
.order:first-child{border-top:0;}
.order-list {
  /* height: 350rpx; */
  padding: 20rpx 32rpx 2rpx;
 /* margin-bottom: 20rpx; */
  /* padding-bottom:2rpx; */
}
.list-top,
.goods-info,
.list-bottom {
  display: flex;
  justify-content: space-between;
}
.list-top,.list-top2 {
  display: flex;
  align-items: center;
  height: 60rpx;
  font-size: 32rpx;
}
.information {
  color: #999999;
  display: block;
  padding-right: 16rpx;
}
.information text{
  word-break: break-all;
}
.long{
  max-width: 560rpx;
  word-break: break-all;
}
.order-sn {
  font-weight: 600;
  display: flex;
}
.list-content {
  font-size: 30rpx;
  padding-right: 12rpx;
}
.list-bottom {
  margin-bottom: 16rpx;
  margin-top: 16rpx;
  height: 46rpx;
  font-size: 30rpx;
  position: relative;
}
.list-tittel{
	height: 32rpx;
	font-family: PingFang-SC-Medium;
	font-size: 30rpx;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0rpx;
	color: #333333;
}
.order-state{
	height: 32rpx;
  font-size:30rpx;
}
.list-btn {
  display: flex;
  /* height: 80rpx; */
  padding:26rpx 32rpx 20rpx;
  flex-direction: row-reverse;
  border-top: #f2f2f2 1rpx solid;
}
.off{
  color: #888888;
}
.list-btn button {
  border: 1rpx solid #e8e8e8;
  border-radius: 4rpx;
  font-size: 28rpx;
  height: 60rpx;
  width: 164rpx;
  margin-left: 22rpx;
  line-height: 60rpx;
  border-radius:8rpx;
  color: #999;
  background-color:#fff;
}
.order-count {
  font-size: 30rpx;
  display: flex;
}
.money {
  color: #ff0036;
  font-size: 34rpx;
}
.fontsBold{font-weight:600;}
.paper-money{
  width: 100%;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  /* font-weight: 300; */
  line-height: 48rpx;
  margin-bottom: 12rpx;
}  
.count-down{
  color: #09bb07;
  font-size: 24rpx;
}
.save{    
  margin-right: 110rpx;
  color: #888888;
}
.pay-btn { 
  border-color:#ff0036!important;
  color: #ff0036!important;
}
.order-list-item {
  width: 100%;   
}
.del-icon{
  vertical-align: middle;
}
.bdnone .order-list-item{
  border: none;
}
.order-null{
  /* width: 750rpx;
  padding-top: 210rpx; */
  text-align: center;
    position: absolute;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%,-50%);
    -moz-transform: translate(-50%,-50%);
    transform: translate(-50%,-50%);  
}
.order-null image{
  width: 310rpx;
  height: 200rpx;
}
.checked-box{
  width: 40rpx;
  height: 40rpx;
  border-radius: 20rpx;
  margin-right: 20rpx;
}
.cart-pro-select{
  width: 40rpx;
  height: 40rpx;
  position: relative;
  left: -2rpx;
  top: -2rpx;
}
.row{  
  width: 100%;
  height: 120rpx;
  font-size: 32rpx;
  position: fixed;
  bottom: 0;
  display: flex;
  justify-content: space-between;
}
.bottom-text{
  width: 70%;
  display: flex;
  justify-content: space-between;
  box-sizing: border-box;
  padding-left: 20rpx;
  text-align: right;
  padding-right: 20rpx;
}
.bottom_line {
  width: 100%;
  height: 2rpx;
  background: lightgray;
}
.bottom_total {
  bottom: 0;
  line-height: 120rpx;
}
.button-red {
  background-color: #ff0036; /* 红色 */
}
.total-button {
  text-align: center;
  display: inline-block;
  color: #fff;
  width: 33%;
  border-radius: 0rpx;
  height: 120rpx;
  line-height: 120rpx;
}
.body .more{
  text-align: center;
  line-height: 30rpx;
  font-size: 28rpx;
  background-color: #f5f5f5;
  color: #999999;
}
.draw{
  background-color: #ff0036;
  color: #fff;
  border: 2rpx solid #e8e8e8;
  border-radius: 4rpx;
  font-size: 28rpx;
  height: 60rpx;
  width: 174rpx;
  margin-left: 22rpx;
  line-height: 54rpx;
}
.reviewed{
  width: 142rpx;
  height: 136rpx;
  position: absolute;
  right: 180rpx;
  top: -80rpx;
}
.price-difference{
  border-top: #dbd8d8 1rpx solid;
  padding: 20rpx 0;
  font-size: 30rpx;
}
.price-difference-title{
  display: flex;
  justify-content: space-between;
}
.difference-pay{
  color: #ff0036;
  height: 80rpx;
  line-height: 80rpx;
}
.search{
  width: 100%;
  box-sizing: border-box;
  padding: 0 20rpx;
  position: relative;
  padding-top: 20rpx;
}
.search input{
  border: 1px solid #dcdfe6;
  border-radius: 60rpx;
  padding: 8rpx 24rpx;
  padding-left: 54rpx;
}
.srarch-icon{
  position: absolute;
  top: 32rpx;
  left: 30rpx;
}
.clear{
  width: 54rpx;
  height: 54rpx;
  position: absolute;
  top: 24rpx;
  right: 30rpx;
  z-index: 9;
  display: flex;
  align-items: center;
  color: #888888;
  border-radius: 20rpx;
}