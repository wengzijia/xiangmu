page {
  background-color: rgb(240, 240, 240);
}
.body view {
  background-color: rgb(255, 255, 255);
}

.enter {
  width: 100%;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: #ff0036;
  color: #fff;
  border-radius: 0;
  padding: 20rpx 0;
}

.containers {
  width: 100%;
  overflow: hidden;
  box-sizing: border-box;
}
.touch-item {
  font-size: 14px;
  height: 140rpx;
  padding: 2rpx 32rpx;
  line-height: 100rpx;
  display: flex;
  /* //均匀排布每个元素 */
  background: #fff;
  border-bottom: 1rpx solid #f5f5f5;
  justify-content: space-between;
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
}
.content {
  width: 400rpx;
  height: 140rpx;
  font-size: 32rpx;
  line-height: 140rpx;
}
.box{
  vertical-align: middle;
  line-height: 130rpx;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked {
  background: #ff0036; 
  border: none;
}
checkbox .wx-checkbox-input.wx-checkbox-input-checked::before{
  color: #fff;
}
checkbox .wx-checkbox-input{
  border-radius: 50%;
}
.order-null{
  width: 750rpx;
  padding-top: 210rpx;
  height: 440rpx;
  background-color: rgb(240, 240, 240);
  text-align: center;
}
.order-null image{
  width: 320rpx;
  height: 234rpx;
}