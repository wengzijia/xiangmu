  height: 120rpx;
  font-size: 32rpx;
  position: fixed;
  bottom: 0;
  display: flex;
  justify-content: space-between;
}

.bottom-text {
  width: 70%;
  display: flex;
  justify-content: space-between;
  box-sizing: border-box;
  padding-left: 20rpx;
  text-align: right;
  padding-right: 20rpx;
}

.bottom_line {
  width: 100%;
