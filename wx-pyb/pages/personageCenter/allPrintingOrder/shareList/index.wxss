.order-null {
  /* width: 750rpx; */
  text-align: center;
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  -moz-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
}

.order-null image {
  width: 310rpx;
  height: 200rpx;
}

