  border-top: 1rpx solid #f5f5f5;
  display: flex;
  justify-content: space-between;
  color: #333;
}

.tabs>text {
  display: inline-block;
  width: 20%;
  text-align: center;
  margin: 0 24rpx;
  color: #999;
}

.grid {
  border-bottom: orangered 4rpx solid;
  color: #ff0036 !important;
}
.order {
  margin: 24rpx;
