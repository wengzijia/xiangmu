page{
  background-color: rgb(240, 240, 240);
}
.body view{
  background-color: rgb(255, 255, 255);
}
.list{
  padding: 0 32rpx;
  /* height: 100rpx; */
  line-height: 94rpx;
  border-bottom: 1rpx solid #f5f5f5;
  /* display: flex;
  justify-content: space-between; */
  position: relative;
  font-size: 28rpx;
}
.list-item,.info,.flex{
  box-sizing: border-box;
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.body .info{
  padding: 0 20rpx;
  font-size: 28rpx;
  border-top: 1rpx solid #f5f5f5;
}
.centent-item{
  width: 23%;
}
.left-item{
  width: 28%;
}
.right-item{
  width: 20%;
}
.greaterThanSign{
  width: 12rpx;
  height: 12rpx;
  border-right: 2rpx solid rgb(100, 100, 100);
  border-top: 2rpx solid rgb(100, 100, 100);
  transform: rotate(45deg);
  position: absolute;
  top:38rpx;
  right:24rpx;
}
.greaterThanSign2{
  width: 16rpx;
  height: 16rpx;
  border-right: 2rpx solid rgb(100, 100, 100);
  border-top: 2rpx solid rgb(100, 100, 100);
  transform: rotate(135deg);
  position: absolute;
  top:16rpx;
  right:26rpx;
}
.tabs{
  width: 92%;
  height: 72rpx;
  margin: 20rpx auto;
  color: #fff;
  display: flex;
  justify-content: space-between;
}
.item{
  display: inline-block;
  text-align: center;
  width: 50%;
  height: 100%;
  line-height: 72rpx;
  border-radius: 12rpx;
}
.active{
  background-color: rgba(255, 255, 255,.7);
  color: #ff0036;
}
.title{
  display: flex;
  justify-content: space-between;
}
.footer{
  position: fixed;
  box-sizing: border-box;
  width: 100%;
  height: 100rpx;
  margin: 0 auto;
  line-height: 100rpx;
  bottom: 0;
  display: flex;
  justify-content: space-between;
  padding: 0 32rpx;
}
