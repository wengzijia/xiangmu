.red{
  color: #ff0036;
}
.order-null{
  width: 750rpx;
  padding-top: 100rpx;
  height: 440rpx;
  background-color: rgb(240, 240, 240);
  text-align: center;
}
.order-null image{
  width: 320rpx;
  height: 234rpx;
}
.tool,.explain{
  font-size: 28rpx;
  color: rgba(0, 0, 0, 0.7);
  position: absolute;
  top: -2rpx;
}
.tool{
  left: 40rpx;
}
.explain{
  right: 40rpx;
}
.picker{
  z-index: 2;
  width: 240rpx;
  margin: 0 auto;
  position: relative;
  text-align: center;
}

.date{
  height: 52rpx;
  line-height: 52rpx;
  font-size: 36rpx;
  margin: 30rpx auto;
  position: relative;
  color: #fff;
}
.date view{
  color: #fff;
}

.greaterThanSign2{
  width: 16rpx;
  height: 16rpx;
  border-right: 2rpx solid rgb(255, 255, 255);
  border-top: 2rpx solid rgb(255, 255, 255);
  transform: rotate(135deg);
  display: inline-block;
  position: relative;
  top: -8rpx;
  left: 8rpx;
}
.total-body{
  width: 85%;
  box-sizing: border-box;
  padding: 0 20rpx;
  height: 164rpx;
  text-align: center;
  border-radius: 20rpx;
  color: rgba(255, 255, 255,1);
  margin: 0 auto 20rpx;
}
.h3{
  font-size: 28rpx;
  color: rgba(255, 255, 255,0.7);
  line-height: 68rpx;
}
.flex{
  display: flex;
  justify-content: space-between;
}
.total-body text{
  font-size: 34rpx;
  font-weight: bold;
}
.input{
  display: inline-block;
  width: 300rpx;
  height: 68rpx;
  box-sizing: border-box;
  padding: 0 20rpx;
  /* border: 1px solid #999; */
  /* border-bottom: 1px solid #999; */
  /* border-radius: 30rpx; */
  margin-top: 20rpx;
}
.withdrawal-box{
  font-size: 32rpx;
  line-height: 100rpx;
  display: flex;
}
.body .withdrawal-btn{
  box-sizing: border-box;
  width: 200rpx;
  height: 68rpx;
  text-align: center;
  background-color: #ff0036;
  color: #fff;
  position: absolute;
  right: 12rpx;
  top: 20rpx;
  padding: 0;
  border: 1rpx solid #ff0036;
  border-radius: 34rpx;
  line-height: 60rpx;
}
.bg-img{
  width: 100%;
  height: 340rpx;
  position: absolute;
  object-fit: contain;
  left: 0;
  top: 0;
  z-index: -1;
}