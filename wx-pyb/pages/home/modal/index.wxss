.modal-wrapp{
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background-color: rgba(0,0,0,0.4);
    z-index: 9999;
  }
  .modal-main{
    margin: auto;
    box-sizing: border-box;
  }

  .modal-button{
    padding-top: 20rpx;
  }
  
  .modal-cancel{
    text-align: center;
    font-size: 50rpx;
  }

  .activity-img{
   width: 593rpx;
    display: block;
    margin: 20rpx auto;

  }

  .modal-herder{
    text-align: center;
    line-height: 50rpx;
  }
 