.personageList2 {
  width: 100%;
  border-top: 1rpx solid #eaeaea;
  position: relative;
}
.personageList2::before {
  position: absolute;
  right: 0;
  bottom: 0;
  left: 0;
  height: 2px;
  background: -webkit-repeating-linear-gradient(
    135deg,
    #ff6c6c 0,
    #ff6c6c 20%,
    transparent 0,
    transparent 25%,
    #1989fa 0,
    #1989fa 45%,
    transparent 0,
    transparent 50%
  );
  background: repeating-linear-gradient(
    -45deg,
    #ff6c6c 0,
    #ff6c6c 20%,
    transparent 0,
    transparent 25%,
    #1989fa 0,
    #1989fa 45%,
    transparent 0,
    transparent 50%
  );
  background-size: 80px;
  content: "";
}
.personageList2 > view {
  border-bottom: 1rpx solid rgb(240, 240, 240);
  display: flex;
  padding: 34rpx 30rpx 34rpx 0;
  margin-left: 30rpx;
  position: relative;
}
