.small_text {
font-size: 28rpx;
margin-right: 40rpx;
margin-left: 95rpx;
}
.item-select {
width: 40rpx;
height: 40rpx;
margin-top: 90rpx;
margin-left: 20rpx;
}
.item-allselect {
width: 40rpx;
height: 40rpx;
margin-left: 20rpx;
}
.item-image {
width: 180rpx;
height: 180rpx;
margin: 20rpx;
}
.bottom_line {
width: 100%;
height: 2rpx;
background: lightgray;
}
.bottom_total {
position: fixed;
display: flex;
flex-direction: column;
bottom: 0;
width: 100%;
height: 120rpx;
line-height: 120rpx;
background: #F4F4F4;
border-top:1rpx solid #e3e3e4;
/* padding-bottom: constant(safe-area-inset-bottom);
padding-bottom: env(safe-area-inset-bottom); */
}
.button-red {
background-color: #FF0036; /* 红色 */
}
button {
position: fixed;
right: 0;
color: white;
text-align: center;
display: inline-block;
font-size: 32rpx;
border-radius: 0rpx;
width: 30%;
height: 120rpx;
line-height: 120rpx;
}
.topbox{
font-size: 30rpx;
padding: 30rpx 20rpx;
}
.evencolor{
float:left;
}

.details text{
word-break:break-all;
}
.cart-tabs{
display: flex;
justify-content: space-between;
align-items: center;
}
.tabs1 {
font-size: 32rpx;
line-height: 86rpx;
background-color: #fff;
display: flex;
justify-content: space-between;
color: #333;
}
.tabs1 > view {
text-align: center;
width: 50%;
}
.tabs1 view text {
display: inline-block;
border-bottom: rgba(255, 255, 255, 0) 4rpx solid;
}
.tabs1 .grid > text {
color: #fff;
}
