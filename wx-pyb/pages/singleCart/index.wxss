page{
  background-color: #F4F4F4;
  font-family: "Helvetica Neue","Hiragino Sans GB","Microsoft YaHei","\9ED1\4F53",Arial,sans-serif;
  font-size: 32rpx;
}
.navigator-hover{
  background: none;
}
.cart-box{
  margin-top: 6rpx;
  height: calc(100vh - 320rpx );
  height:calc(100vh - 320rpx) ;
}
.cart-list{
  position: relative;
  padding: 20rpx 20rpx 20rpx 86rpx;
  margin: 0 20rpx 20rpx;
  border-radius: 12rpx;
  background: #fff;

  z-index:0;
  float:left;
}
.cart-list .cart-pro-select{
  position: absolute;
  left: 20rpx;
  top: 20rpx;
  width: 45rpx;
  height: 45rpx;
}

.cart-list .cart-thumb{
  position: absolute;
  top: 20rpx;
  left: 85rpx;
  width: 185rpx;
  height: 185rpx;
}
.cart-list .cart-pro-name{
  display: inline-block;
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  overflow: hidden;
}
.cart-con{
width: 100%;
color: #A6A6A6;
font-size: 30rpx;
float: left;
padding-bottom:20rpx;
}
.cart-list .cart-pro-price{
  /* display: inline-block; */
  /* float: left; */
  color: #FF0036;
  font-size: 35rpx;
  font-weight: bold;
}
.borra{
  border: 1px solid #EBEBEB;
  border-radius: 6rpx;
}
.cart-list .cart-count-box{
  /* position: absolute; */
  margin-right: 20rpx;
  /* bottom: 30rpx; */
  font-weight: bold;
  /* line-height: 54rpx; */
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 70rpx;
}
.cart-count-down,.cart-count-add{
  /* float: left; */
  text-align: center;
  font-size: 34rpx;
  width: 65rpx;
  height: 55rpx;
  line-height: 52rpx;
  /* background: #F5F5F5; */
  color: #aaa;
  /* font-weight: bold; */
}
.cart-count-num{
  width: 150rpx;
  /* height: 55rpx; */
  float: left;
  text-align:center;
  color: #333;
  /* padding-top: 10rpx; */
  border-left: 1rpx solid #EBEBEB;
  border-right: 1rpx solid #EBEBEB;
  background: #FCFCFC;
  /* margin:0 10rpx */
  z-index:0;
}
.cart-list .no-border{
border: none;
}
.cart-del{
  position: absolute;
  right: 20rpx;
  bottom: 20rpx;
  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 44rpx;
}
.details{
font-size:30rpx;
margin-top: 10rpx;
}
.cart-footer{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  padding:0 100rpx 0 80rpx;
  box-sizing: border-box;
  background: #AB956D;
  color: #fff;
}
.total-select{
  position: absolute;
  left: 20rpx;
  top: 25rpx;
  width: 45rpx;
  height: 45rpx;
}
.order-icon{
  position: absolute;
  right: 40rpx;
  top: 25rpx;
  width: 45rpx;
  height: 45rpx;
}
.order-icon image,.order-icon navigator{
display: block;
width: 45rpx;
height: 45rpx;
}
.cart-toatl-price{
  float: right;
  width: 120rpx;
}
.cart-noBox{
position:absolute;
top:50%;
left:50%;
-webkit-transform: translate(-50%,-50%);
-moz-transform: translate(-50%,-50%);
transform:translate(-50%,-50%);
}
.cart-no-data{
  padding:20rpx 0;
  color: #999;
  text-align: center;
font-size:26rpx;
}
.cart-no-data image{
width: 280rpx;
height: 200rpx;
}

page::after{
  content: '';
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 1px;
  background-color: #f5f5f5;
  z-index: 9999;
}

