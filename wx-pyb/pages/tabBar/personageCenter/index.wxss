page{
  background-color: rgb(248, 248, 248);
  width: 100%;
}
.personage>view{
  background-color:#fff;
}
.menu_img {
  position: absolute;
  height: 45rpx;
  width: 180rpx;
  top: 50%;
  transform: translateY(-50%);
  right: 50rpx;
  text-align: center;
  line-height: 45rpx;
}
.menu_img_title {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 175rpx;
  color: #E9333F;
  /* transition: inherit; */
  transform: translate(-50%,-50%);
  font-size: 26rpx;
}
.flex_between {
  display: flex;
  justify-content: space-between;
  padding: 18rpx 18rpx 0 18rpx;
}
.left {
  font-weight: 700;
  color: #333333;
  font-size: 15px;
}
.right {
  font-weight: 400;
  color: #777777;
  font-size: 12px;
}
.personage .herder{
  position: relative;
  height: 498rpx;
  margin-bottom:36rpx;
  background: url(data:image/png;base64,)no-repeat;
  background-size:100%;
}

.personage .herder .navName{
 /* text-align: center;
 color: #fff;
 font-size: 38rpx;
 padding-top: 70rpx; */
}
.personage .userinfo{
  width: 100%;
  display: flex;
  padding-left: 40rpx;
  box-sizing: border-box;
  color: #fff !important;
  
   /* margin-bottom: 20rpx; */
}
.personage .userinfo .userAvatarUrl{
  width:128rpx;
  height:128rpx;
  border-radius:50%;
  background-color: #ffffff;
  /* border:4rpx solid #fff; */
  overflow: hidden;
}
.userAvatarUrl image{
  width: 100%;
  height:100%
}
.nickname{
  color: #fff !important;
}
.personage .userinfo .nickname{
 display: inline-block;
 font-family: PingFang-SC-Medium;
 font-size:32rpx;
 margin-left:28rpx;
 flex:1;
 color:#333333;
}
.personage .userinfo .ac-icon-set{
  line-height:138rpx;
 margin-right:50rpx;
 font-size:40rpx;
}
.orderListStatusBtn{
  margin: 0 34rpx;
  /* position: absolute; */
  /* bottom: -90rpx;*/
 /* width: 94%; 
   left: 3%; */
  height: 174rpx;
  background-color: #fff;
  border-radius: 15rpx;
  box-shadow: 0 2px 10px 0 rgb(0 0 0 / 10%);
  border:1rpx solid #eaeaea;
}
.orderListItem{
  width: 25%;
  text-align: center;
  font-size:26rpx;
  /* padding: 20rpx 0; */
}
.orderListStatusBtn>view image{
  width:60rpx;
  height:60rpx;
  /* margin-bottom:4rpx; */
}

.personageList1{
  margin-bottom:40rpx;
}

.personageList1,.personageList2{
	border-top:1rpx solid #eaeaea;
	border-bottom:1rpx solid #eaeaea;
}
.personageList1>view,.personageList2>view{
  border-bottom:1rpx solid rgb(240, 240, 240);
 display:flex;
  padding:34rpx 30rpx 34rpx 0;
  margin-left:30rpx;
    position: relative;
}
.personageList1>view:last-child,.personageList2>view:last-child{
	border-bottom:0;
}
.personageList1>view>text,.personageList2>view>text{
  flex:1;
  font-size:32rpx;
  color: #333333;
}
.personageList2 .servicePhone{
  color:rgb(172, 170, 170);
  font-size:28rpx;
  margin-right:30rpx;
}

.greaterThanSign{
    width: 15rpx;
    height: 15rpx;
    border-right: 2rpx solid rgb(172, 170, 170);
    border-top: 2rpx solid rgb(172, 170, 170);
    transform: rotate(45deg);
    position: absolute;
    top:45rpx;
    right:35rpx;
}

.onLineService{
  position:fixed;
  width:120rpx;
  height:120rpx;
  right:30rpx;
  bottom:90rpx;
}
